{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "TypeScript types-only library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["ES2015"],
    "module": "ESNext",
    "target": "ES6",
    "importHelpers": true,
    "sourceMap": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "jsx": "react",
    "skipLibCheck": true,
    "noEmit": true
  }
}
