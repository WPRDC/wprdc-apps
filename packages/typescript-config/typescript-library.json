{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "TypeScript types-only library",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["dom", "esnext"],
    "importHelpers": true,
    "sourceMap": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "jsx": "react",
    "skipLibCheck": true
  }
}
